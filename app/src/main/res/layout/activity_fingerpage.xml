<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/grad"
    tools:context=".fingerpage">


    <TextView
        android:id="@+id/headinglabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/fingerprintimage"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="154dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:gravity="center"
        android:text="@string/s3"
        android:textColor="#E3EFF1"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/paralabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/fingerprintimage"
        android:layout_centerInParent="true"
        android:textColor="#D2DCEA"
        android:text="@string/s2" />

    <ImageView
        android:id="@+id/fingerprintimage"
        android:layout_width="153dp"
        android:layout_height="163dp"
        android:layout_centerInParent="true"
        android:contentDescription="@string/image"
        app:srcCompat="@drawable/finger7"
      />

    <TextView
        android:id="@+id/xtext"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:padding="22dp"
        android:layout_marginTop="150dp"
        android:text="X :"
        android:textColor="#0C0C0C"
        android:textSize="18sp"
        android:textStyle="bold" />


    <TextView
        android:id="@+id/ytext"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:padding="22dp"
        android:layout_marginTop="250dp"
        android:text="Y :"
        android:textColor="#0C0C0C"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/ztext"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:layout_below="@id/ytext"
        android:layout_marginTop="35dp"
        android:padding="20dp"
        android:text="Z : "
        android:textColor="#0C0C0C"
        android:textSize="18sp"
        android:textStyle="bold" />

    <ListView
        android:layout_width="319dp"
        android:layout_height="279dp"
        android:id="@+id/listView"
        android:layout_marginTop="420dp" />



    <TextView
        android:id="@+id/id"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:layout_marginTop="35dp"
        android:padding="20dp"
        android:text="id :"
        android:textColor="#0C0C0C"
        android:textSize="18sp"
        android:textStyle="bold" />


</RelativeLayout>